name "unit-threaded"
description "Advanced multi-threaded unit testing framework with minimal to no boilerplate using built-in unittest blocks"
homepage "https://github.com/atilaneves/unit-threaded"
authors "Atila Neves"
copyright "Copyright Â© 2013, Atila Neves"
license "BSD 3-clause"
x:ddoxFilterArgs "--unittest-examples" "--min-protection=Protected"
x:ddoxTool "scod"
dependency "unit-threaded:mocks" version=">=0.0.0"
dependency "unit-threaded:assertions" version=">=0.0.0"
dependency "unit-threaded:property" version=">=0.0.0"
dependency "unit-threaded:runner" version=">=0.0.0"
dependency "unit-threaded:integration" version=">=0.0.0"
dependency "unit-threaded:exception" version=">=0.0.0"
dependency "unit-threaded:from" version=">=0.0.0"
targetType "library"
configuration "library" {
	targetType "library"
}
configuration "unittest" {
	targetType "executable"
	targetName "ut_pass"
	mainSourceFile "example/example_pass.d"
	dflags "-preview=dip25" "-preview=dip1000" "-preview=dip1008"
	sourcePaths "source" "gen/source" "tests/unit_threaded" "tests/examples/pass"
	versions "testing_unit_threaded"
	importPaths "source"
}
configuration "unittest-unthreaded" {
	targetType "executable"
	targetName "ut_pass"
	mainSourceFile "example/example_pass.d"
	dflags "-preview=dip25" "-preview=dip1000" "-preview=dip1008"
	sourcePaths "source" "gen/source" "tests/unit_threaded" "tests/examples/pass"
	versions "testing_unit_threaded" "unitUnthreaded"
}
configuration "unittest-light" {
	targetType "executable"
	targetName "ut_pass"
	mainSourceFile "example/example_pass.d"
	dflags "-preview=dip25" "-preview=dip1000" "-preview=dip1008"
	sourcePaths "source" "gen/source" "tests/unit_threaded" "tests/examples/pass"
	versions "testing_unit_threaded" "unitThreadedLight" "unitUnthreaded"
}
configuration "unittest-fail" {
	targetType "executable"
	targetName "ut_fail"
	mainSourceFile "example/example_fail.d"
	sourcePaths "tests/examples/pass" "tests/examples/fail/"
	versions "testing_unit_threaded"
}
configuration "gen_ut_main" {
	targetType "executable"
	targetName "gen_ut_main"
	mainSourceFile "gen/gen_ut_main.d"
	sourcePaths "gen/source"
	importPaths "gen/source"
}
subPackage "./subpackages/from"
subPackage "./subpackages/exception"
subPackage "./subpackages/assertions"
subPackage "./subpackages/runner"
subPackage "./subpackages/mocks"
subPackage "./subpackages/integration"
subPackage "./subpackages/property"
